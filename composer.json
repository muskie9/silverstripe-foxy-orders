{
    "name": "dynamic/silverstripe-foxy-orders",
    "description": "Add order history to your SilverStripe Foxy store.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Foxy.io",
        "ecommerce",
        "orders"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "dynamic/silverstripe-foxy": "^1.0",
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripeModule\\Skeleton\\": "src/",
            "SilverStripeModule\\Skeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
