{
    "name": "dynamic/silverstripe-foxy-orders",
    "description": "Add order history to your SilverStripe Foxy store.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Foxy.io",
        "ecommerce",
        "orders"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "dynamic/silverstripe-foxy": "^1.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "theseer/phpdox": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Foxy\\": "src/",
            "Dynamic\\Foxy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
